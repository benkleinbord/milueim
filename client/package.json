{
  "name": "milueim",
  "version": "1.0.0",
  "description": "milueim is taking care for each other ",
  "scripts": {
    "prestart": "babel-node tools/startMessage.js",
    "start": "set NODE_ENV=dev&& npm-run-all --parallel open:src lint:watch",
    "start:mac": "NODE_ENV=dev npm-run-all --parallel open:src lint:watch",
    "start:qa": "set NODE_ENV=qa&& npm-run-all --parallel open:src lint:watch",
    "start:prod": "set NODE_ENV=prod&& npm-run-all --parallel open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist lint build:html",
    "build": "babel-node tools/build.js",
    "mockApi": "babel-node mockApi/server.js",
    "build:dev": "set NODE_ENV=dev&& npm run build",
    "build:qa": "set NODE_ENV=dev&& npm run build",
    "build:qa-bamboo": "NODE_ENV=qa npm run build",
    "build:prod-bamboo": "NODE_ENV=prod npm run build",
    "build:prod": "set NODE_ENV=prod&& npm run build"
  },
